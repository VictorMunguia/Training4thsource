
public class toString3 {
	private String name;
	private toString cumpleanos;//Este atributo es el tipo toString que es la clase que icimos previamente
	
	public toString3(String nombre, toString fecha) {
		this.name = nombre;
		this.cumpleanos = fecha;
	}
	
	
	public String toString() {
		return String.format("My name is %s, my birthday is %s" ,  nombre,cumpleanos)
	}

}
