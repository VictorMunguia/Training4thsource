
public class metodosMatematicosClases {
	public static void main(String[] args) {
		System.out.println(Math.abs(-26.7));//Retorna el valor absoluto, negativo lo vuelve positivo.
		System.out.println(Math.ceil(7.2));//Redondea hacia arriba
		System.out.println(Math.floor(7.2));//Redondea hacia abajo
		System.out.println(Math.max(-26.7));
	}
}
