
public class multiplesConstructores {
	private int hora;
	private int minuto;
	private int segundo;
	
	public multiplesConstructores() {
		
	}
	
	
	public multiplesConstructores(int h) {
		hora = h;
	}
	
	public multiplesConstructores(int h, int m) {
		hora = h;
		minuto = m;
	}
	
	public multiplesConstructores(int h, int m, int s) {
		hora = h;
		minuto = m;
		segundo = s;
	}
	
	
	
	

}
